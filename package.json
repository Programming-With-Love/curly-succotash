{
  "name": "curly-succotash",
  "description": "a neweast and beautiful blog starter,build by gatsby",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:zidoshare/curly-succotash.git",
  "author": "zido <wuhongxu1208@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:js && npm run lint:css",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "lint:md": "remark .",
    "lint:js": "xo",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:scss": "stylelint src/**/*.scss --syntax scss",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@material-ui/core": "^3.7.0",
    "@material-ui/icons": "^3.0.1",
    "@types/jss": "^9.5.7",
    "@types/lodash": "^4.14.119",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.7",
    "@types/react-jss": "^8.6.0",
    "@types/react-redux": "^6.0.11",
    "babel-eslint": "^9",
    "codeclimate-test-reporter": "^0.5.1",
    "disqus-react": "^1.0.5",
    "gatsby": "^2.0.73",
    "gatsby-plugin-jss": "^2.0.5",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-remark-prismjs": "^3.1.4",
    "gatsby-transformer-json": "^2.1.6",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "react-typography": "^0.16.18",
    "redux": "^4.0.1",
    "semantic-ui-react": "^0.84.0",
    "typography": "^0.16.18"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-notes": "^4.1.3",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-knobs": "^4.0.0",
    "@types/storybook__addon-notes": "^4.0.0",
    "@types/storybook__addon-options": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^6",
    "babel-loader": "^7.0.0",
    "eslint": "^5.10.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-config-xo-react": "0.11.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.1.0",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-remark-autolink-headers": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.12",
    "jest": "^23.6.0",
    "prismjs": "^1.15.0",
    "redux-devtools-extension": "^2.13.7",
    "webpack": "^4.28.0",
    "xo": "^0.23.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
